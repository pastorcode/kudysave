{
	"name": "codefii/codefii",
	"description": "An easy to use php framework for building web applications.",
	"keywords": [
		"phpframework",
		"codefii",
		"codefiiphp"
	],
	"homepage": "https://codefii.com",
	"type": "project",
	"license": "MIT",
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/codefii/codefii"
		}
	],
	"authors": [
		{
			"name": "Prince E. Darlington",
			"email": "ekeminyd@gmail.com",
			"homepage": "https://twitter.com/iamcoderisk"
		}
	],
	"require": {
		"php": "^5.6 || ^7.0",
		"codefii/framework": "1.2",
		"phpunit/phpunit": "3.7.*",
		"vlucas/phpdotenv": "^2.4"
	},
	"autoload": {
		"psr-4": {
			"App\\": "App/",
			"Dotenv\\": "src/",
			"Codefii\\": "src/Network/"
		}
	},
	"extra": {
		"codefii": {
			"dont-discover": []
		}
	},
	"config": {
		"preferred-install": "dist",
		"sort-packages": true,
		"optimize-autoloader": true
	}
}
